{
  "$schema": "../../../../../../type-schema.json",
  "Status": {
    "kind": "enum",
    "description": "User status",
    "values": [
      {
        "value": "active",
        "description": "User is active"
      },
      {
        "value": "inactive",
        "description": "User is inactive"
      }
    ]
  },
  "UserId": {
    "kind": "alias",
    "description": "Unique identifier for a user",
    "target": "string",
    "format": "uuid"
  },
  "StringMap": {
    "kind": "map",
    "description": "A string map",
    "keyType": "string",
    "valueType": {
      "type": "string"
    }
  },
  "Tags": {
    "kind": "array",
    "description": "A list of tags",
    "itemType": {
      "type": "string"
    }
  },
  "User": {
    "kind": "object",
    "description": "User entity",
    "fields": [
      {
        "name": "id",
        "description": "User ID",
        "$ref": "UserId"
      },
      {
        "name": "createdAt",
        "description": "When the user was created",
        "type": "string",
        "format": "date-time"
      },
      {
        "name": "status",
        "description": "User status",
        "$ref": "Status"
      },
      {
        "name": "tags",
        "description": "User tags",
        "items": {
          "type": "string"
        }
      }
    ]
  }
}
